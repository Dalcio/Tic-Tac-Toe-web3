# ====================================
# Environment Variables for Web3 Tic-Tac-Toe
# ====================================
# Copy this file to .env and fill in your values

# Your wallet private key (DO NOT commit this to git!)
# Get this from MetaMask: Account Details > Export Private Key
PRIVATE_KEY=your_wallet_private_key_here

# RPC URLs for testnets
# You can get free RPC URLs from:
# - Alchemy: https://www.alchemy.com/
# - Infura: https://infura.io/
# - QuickNode: https://www.quicknode.com/
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your-api-key
MUMBAI_RPC_URL=https://polygon-mumbai.g.alchemy.com/v2/your-api-key

# API keys for contract verification (optional)
# Get from https://etherscan.io/apis
ETHERSCAN_API_KEY=your_etherscan_api_key
# Get from https://polygonscan.com/apis
POLYGONSCAN_API_KEY=your_polygonscan_api_key

# Contract address (will be set after deployment)
REACT_APP_CONTRACT_ADDRESS=your_deployed_contract_address
